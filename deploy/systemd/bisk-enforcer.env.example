# Copy to: /etc/bisk/bisk-enforcer.env  (per host)
DJANGO_SETTINGS_MODULE=bisk.settings
PYTHONUNBUFFERED=1
TZ=Asia/Baghdad

# BISK tuning
ENFORCER_INTERVAL_SECONDS=15
ENFORCER_LOCK_FILE=/tmp/bisk_enforcer.lock

# PATH (optional; helpful so nvidia-smi/ffmpeg are found by systemd)
PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/bin
